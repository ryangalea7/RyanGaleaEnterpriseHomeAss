<!--Partialview being used by the layout-->

@if (TempData["feedback"] != null)
{
    <div class="alert alert-primary" role="alert">
        @TempData["feedback"]
    </div>
}

@if (TempData["warning"] != null)
{
    <div class="alert alert-warning" role="alert">
        @TempData["warning"]
    </div>
}